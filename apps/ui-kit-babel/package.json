{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "ui-kit-babel",
  "type": "module",
  "version": "0.0.1",
  "description": "Пакет стилизованных компонентов для проектов mentalhealth",
  "keywords": [
    "ui library"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./main.css": {
      "import": "./dist/main.css",
      "require": "./dist/main.css"
    }
  },
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.x",
    "bun": ">=1.x"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "cross-env NODE_ENV=lib babel src --out-dir dist --source-maps --extensions \".ts\",\".tsx\" --copy-files --no-copy-ignored --out-file-extension .mjs",
    "-------------------------------------------------------------------------------": "",
    "storybook:build": "storybook build",
    "--------------------------------------------------------------------------------": "",
    "typecheck": "tsc -p tsconfig.app.json --noEmit ",
    "----------------------------------------------------------------------------------": "",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/vue3": "^8.0.0",
    "@storybook/vue3-vite": "^8.0.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "20.11.28",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/babel-plugin-jsx": "^1.2.1",
    "@vue/tsconfig": "^0.5.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-import-extension": "1.5.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-define": "^2.1.4",
    "babel-polyfill": "^6.26.0",
    "clean-package": "^2.2.0",
    "cross-env": "^7.0.3",
    "esno": "^4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.72.0",
    "storybook": "^8.0.0",
    "typescript": "5.4.2",
    "vite": "5.1.6",
    "vite-plugin-dts": "3.7.3",
    "vue": "^3.4.21"
  },
  "clean-package": {
    "remove": [
      "scripts",
      "devDependencies",
      "publishConfig",
      "clean-package",
      "packageManager",
      "lint-staged",
      "config"
    ]
  }
}
